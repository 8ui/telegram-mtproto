{
  "presets": [
    // ["es2015", { "modules": false }]
  ],
  "plugins": [
    "transform-flow-strip-types",
    "transform-class-properties",
    ["transform-async-to-module-method", {
      "module": "bluebird",
      "method": "coroutine"
    }],
    ["transform-object-rest-spread", { "useBuiltIns": true }],
    "closure-elimination"
  ],
  "env": {
    "commonjs": {
      "plugins": [
        "transform-es2015-modules-commonjs"
      ]
    }
  }
}