[ignore]
<PROJECT_ROOT>/**/lib/**/*.*
<PROJECT_ROOT>/**/es/**/*.*
<PROJECT_ROOT>/**/dist/*.*
<PROJECT_ROOT>/**/logs/*.*
<PROJECT_ROOT>/**/examples/*.*
<PROJECT_ROOT>/**/.vscode/*.*
<PROJECT_ROOT>/**/typings/**/*.*
<PROJECT_ROOT>/**/*.draft.js

[include]
<PROJECT_ROOT>/telegram-mtproto/src/**/*.js
<PROJECT_ROOT>/mtproto-logger/src/**/*.js
<PROJECT_ROOT>/mtproto-shared/src/**/*.js
<PROJECT_ROOT>/mtproto-storage-fs/src/**/*.js
<PROJECT_ROOT>/../flow-typed/**/*.js

[options]
suppress_comment=\\(.\\|\n\\)*\\$FlowIssue
suppress_type=$FlowIssue
unsafe.enable_getters_and_setters=true
emoji=true
esproposal.decorators=ignore
module.use_strict=true
strip_root=true
