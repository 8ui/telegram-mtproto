{
  "name": "mtproto-classify",
  "version": "1.0.0",
  "description": "telegram-mtproto incoming message classifier written on purescript",
  "main": "index.js",
  "scripts": {
    "postinstall": "bower install",
    "test": "echo \"Error: no test specified\" && exit 1",
    "watch": "pulp -w run",
    "build:cjs": "pulp build --to lib",
    "build:bundle": "pulp build -O --to dist/pure-tl.js",
    "build": "npm run build:cjs && npm run build:bundle",
    "rollup": "pulp build --source-maps && rollup --config"
  },
  "author": "Zero Bias",
  "license": "MIT",
  "dependencies": {
    "bower": "^1.8.2",
    "purescript": "^0.11.6",
    "react": "^16.0.0",
    "react-dom": "^16.0.0"
  },
  "devDependencies": {
    "babel-eslint": "^8.0.0",
    "eslint": "^4.7.2",
    "eslint-plugin-babel": "^4.1.2",
    "eslint-plugin-flowtype": "^2.36.0",
    "flow-bin": "^0.55.0",
    "pulp": "^12.0.1",
    "rollup": "^0.50.0",
    "rollup-plugin-purs": "^1.0.38",
    "typescript": "^2.5.2"
  }
}
